<div class="entry-navbar navbar">
    <ul style="list-style: none;">
      <br/>
     
      <li class="logo">
      dreambook
      </li>
      <li class="link-signup">
        <%= link_to 'sign up', new_user_registration_path %>
      </li>
    </ul>
</div>


<div class="sessions-new">
<h1 class="regist-header">log in</h1>
<br/>
<%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
  <div class="field font-input email-regist">
    <%= f.email_field :email, autofocus: true, autocomplete: "email", placeholder: 'Email' %>
  </div>

  <div class="field font-input password-regist">
    <%= f.password_field :password, autocomplete: "off", placeholder: "Password" %>
  </div>

  <% if devise_mapping.rememberable? -%>
    <div class="field remember-login">
      <%= f.check_box :remember_me, checked: true, hidden: true %>
    </div>
  <% end -%>

  <div class="actions registration-button">
    <%= f.submit "log in" %>
  </div>
<% end %>

</div>

